{
  "manifest_version": 3,
  "name": "Tracy",
  "description": "chrome extension made using React",
  "version": "1.0.0",
  "action": {
    "default_title": "React extension",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["storage", "tabs", "activeTab", "history", "background"]
}
